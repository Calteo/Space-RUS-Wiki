{
  "build": {
    "content": [
      {
        "files": [
          "about/**.md",
          "about/**/toc.yml",
          "faq/**.md",
          "faq/**/toc.yml",
          "learn/**.md",
          "learn/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadata": {
      "_appTitle": "SpaceX RUS Wiki",
      "_appFooter": "Some footer needed here.",
      "__global.improveThisDoc": "Make it a better wiki",
      "__global.backToTop": "Beam me up Scotty!"
    },
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default"
    ],
    "postProcessors": [],    
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
    }
  }